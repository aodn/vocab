
<skos:TopConcept rdf:about="<%= @topConcept %>">

    <% for @object in query_rdf_objects( 'skos:prefLabel', @topConcept ) %>
    <skos:prefLabel><%= @object %></skos:prefLabel>
    <% end %>

    <% for @object in query_rdf_objects( 'skos:definition', @topConcept ) %>
    <skos:definition><%= @object %></skos:definition>
    <% end %>

    <% for @object in query_rdf_objects( 'dc:source', @topConcept ) %>
    <dc:source><%= @object %></dc:source>
    <% end %>

    <% for @object in query_rdf_objects( 'dcterms:issued', @topConcept ) %>
    <dcterms:issued><%= @object %></dcterms:issued>
    <% end %>

    <% for @object in query_rdf_objects( 'skos:inScheme', @topConcept ) %>
    <skos:inScheme><%= @object %></skos:inScheme>
    <% end %>

    <% for @object in query_rdf_objects( 'skos:narrower', @topConcept ) %>
    <skos:narrower><%= @object %></skos:narrower>
    <% end %>

</skos:TopConcept>

