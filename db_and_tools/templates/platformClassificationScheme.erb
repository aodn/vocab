
<%= render "header.erb" %>

<% @conceptScheme = 'http://vocab.aodn.org.au/platform_classes/1' %>
<% @lang = 'xml:lang="en"' %>

<%= render "concept_scheme_pp.erb" %>

<% for @concept in query_sql_subject( %{
	select csc.name as subject
	  from classification_scheme_category csc
	  join classification_scheme cs on csc.classification_scheme_id = cs.id
	 where cs.name = '#{@conceptScheme}' },
	nil )
%>
<%= render "concept.erb" %>
<% end %>

<%= render "footer.erb" %>

