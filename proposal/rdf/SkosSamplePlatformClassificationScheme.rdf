<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/25/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:narrower rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/1"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Autonomous Underwater Vehicle</skos:prefLabel>
  <skos:definition>A free-roving platform operating in the water column with propulsion but no human operator onboard (e.g. Autosub Glider).</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>

<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/17/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/2"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Coastal Structure</skos:prefLabel>
  <skos:definition> A fixed man-made structure permanently linked to land with access to water at all states of the tide to which instrumentation may be attached (e.g. pier) </skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/46/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/3"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Drifting Subsurface Profiling Float</skos:prefLabel>
  <skos:definition> An unmanned instrumented platform drifting freely in the water column that periodically makes vertical traverses through the water column (e.g. Argo float)</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
	</skos:Concept>	
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/42/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/3"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Drifting Surface Float</skos:prefLabel>
  <skos:definition> An unmanned instrumented platform operating on the surface of the water column often attached to a drogue to track currents rather than winds (e.g. Argos buoy).</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>	
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/36/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/5"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Fishing Vessel</skos:prefLabel>
  <skos:definition>A platform operating on the surface of the water column whose primary purpose is the commercial harevsting of fish or shellfish but may be engaged in scientific activities such as fish stock surveys or mooring deployments and recoveries.</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
	</skos:Concept>
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/11/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/2"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Fixed Benthic Node</skos:prefLabel>
  <skos:definition>A collection of oceanographic instruments mounted at a fixed position on the seabed (e.g. POL Monitoring Platform seabed ADCP)</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/73/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/6"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Flightless Bird</skos:prefLabel>
  <skos:definition>A bird that is unable to fly with the ability to exist within the water column (e.g. penguin).</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/64/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/7"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Geostationary Orbiting Satellite</skos:prefLabel>
 <skos:definition>A vehicle operating beyond the Earth''s atmosphere without human occupants that orbits the Earth at the same rate as the Earth''s rotation keeping it over a fixed location on the Earth''s surface.</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/14/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/2"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Land Onshore Structure</skos:prefLabel>
 <skos:definition>A fixed man-made structure on land to which instrumentation may be attached (e.g. meteorological tower)</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/77/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/6"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Land-Sea Mammals</skos:prefLabel>
  <skos:definition>A mammal that exists both on land and within the water column. Includes seals sealions sea-otters and walruses.</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/26/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/1"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Lowered Unmanned Submersible</skos:prefLabel>
  <skos:definition>An unmanned platform lowered and raised vertically by a cable from the mothership.  Includes any type of profiling sensor mounting such as CTD frames profiling radiometers and instrumented nets.</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>

<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/41/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/10"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Moored Surface Buoy</skos:prefLabel>
  <skos:definition>An unmanned instrumented platform operating on the surface of the water column loosely tethered to the seafloor to maintain a fixed position (e.g. ODAS buoy)</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/48/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/10"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Mooring</skos:prefLabel>
  <skos:definition>A tethered collection of oceanographic instruments at a fixed location that may include seafloor mid-water and surface components.</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>	
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/16/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/2"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Offshore Structure</skos:prefLabel>
  <skos:definition>A fixed (for the duration of the measurements) man-made structure away from the coast to which instrumentation may be attached (e.g. oil rig gas rig or jack-up barge)</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>		
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/65/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/7"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Orbiting Satellite</skos:prefLabel>
  <skos:definition>A vehicle operating beyond the Earth''s atmosphere without human occupants that orbits the Earth at a different rate to the Earth''s rotation so it moves over the Earth''s surface.</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>	
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/70/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/6"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Organism</skos:prefLabel>
  <skos:definition>A living creature carrying instruments or collecting samples.</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>		
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/31/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/5"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Research Vessel</skos:prefLabel>
  <skos:definition>A platform of any size operating on the surface of the water column in unpredictable locations that is specifically equipped manned and operated for scientific usually oceanographic research.</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>			
</skos:Concept>		
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/37/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/5"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Self-propelled Boat</skos:prefLabel>
  <skos:definition>A small self-propelled platform operating on the surface of the water column in unpredictable locations that is smaller than a ship but too large to easily remove from the water.</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>	
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/30/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/5"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Ship</skos:prefLabel>
  <skos:definition>A large platform operating on the surface of the water column. Objective definitions for guidelines: >50m length (EU) >100 foot length (USA) >300 GRT weight (SOLAS). Subjective definition: a ship is a vessel big enough to carry a boat.</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>	
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/32/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/5"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Vessel Of Opportunity</skos:prefLabel>
  <skos:definition>A platform for purpose of commerce of any size operating on the surface of the water column in unpredictable locations that regularly collects scientific (oceanographic and meteorological) data (e.g. an instrumented cargo vessel).</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>		
	
<skos:Concept rdf:about="http://vocab.nerc.ac.uk/collection/L06/current/35/">	
  <skos:inScheme rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/SchemeNumber1"/>
  <skos:broader rdf:resource="http://vocab.aodn.org.au/PlatformClassificationScheme/PlatformConcept/5"/>		
  <skos:inScheme rdf:resource="http://environment.data.gov.au/aodn/def/Platform"/>
  <skos:prefLabel xml:lang="en">Vessel Of Opportunity On A Fixed Route</skos:prefLabel>
  <skos:definition>A platform repeatedly following a predictable fixed track on the surface of the water column that collects scientific (oceanographic and meteorological) data (e.g. an instrumented ferry).</skos:definition>
  <dc:source>L06 SeaVox Platform Categories</dc:source>
  <dc:publisher>(BODC) NERC Vocabulary Server</dc:publisher>		
</skos:Concept>	
	
